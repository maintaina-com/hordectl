{
    "name": "horde/hordectl",
    "description": "Scenario deployer library",
    "type": "library",
    "homepage": "https://www.horde.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Ralf Lang",
            "email": "lang@b1-systems.de",
            "role": "lead"
        }
    ],
    "time": "2022-11-04",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/util": "^3 || dev-FRAMEWORK_6_0",
        "horde/injector": "^3 || dev-FRAMEWORK_6_0",
        "horde/cli_modular": "^3 || dev-FRAMEWORK_6_0",
        "horde/cli": "^3 || dev-FRAMEWORK_6_0",
        "horde/argv": "^3 || dev-FRAMEWORK_6_0",
        "horde/yaml": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0",
        "phpunit/phpunit": "^9 || ^10"
    },
    "suggest": {},
    "autoload": {
        "psr-4": {
            "Horde\\Hordectl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Hordectl\\Test\\": "test/"
        }
    },
    "bin": [
        "bin/hordectl"
    ],
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}